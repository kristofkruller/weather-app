{
  "version": "2.0.0",
  "tasks": [
      {
          "label": "Run dev server",
          "type": "shell",
          "command": "npm run dev",
          "isBackground": true,
          "presentation": {
              "reveal": "always",
              "panel": "new"
          },
          "problemMatcher": {
              "owner": "custom",
              "pattern": {
                  "regexp": ".*",
                  "file": 1,
                  "location": 2,
                  "message": 3
              },
              "background": {
                  "activeOnStart": true,
                  "beginsPattern": ".*",
                  "endsPattern": ".*"
              }
          }
      }
  ]
}
